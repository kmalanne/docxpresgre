{
  "name": "docxpresgre",
  "version": "1.0.0",
  "description": "Starter for Express/Postgres API using Docker containers",
  "author": "Kai-Mikael Alanne",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "start": "nodemon src/server.js --exec babel-node",
    "lint": "./node_modules/.bin/eslint src/**",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill",
    "migrate": "NODE_ENV=test ./node_modules/.bin/knex migrate:latest --knexfile src/db/knexfile.js",
    "seed": "NODE_ENV=test ./node_modules/.bin/knex seed:run --knexfile src/db/knexfile.js",
    "db_up": "npm run migrate && npm run seed"
  },
  "keywords": [
    "node",
    "express",
    "postgres",
    "docker"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "express-validation": "^1.0.2",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.1",
    "knex": "^0.12.9",
    "pg": "^6.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalanne/docxpresgre.git"
  },
  "homepage": "https://github.com/kmalanne/docxpresgre#readme",
  "bugs": {
    "url": "https://github.com/kmalanne/docxpresgre/issues"
  }
}